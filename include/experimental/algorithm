                          InputIterator first, Size n,
                          OutputIterator result)
  {
    static_assert(experimental::parallel::is_execution_policy<ExecutionPolicy>::value,
                  "exec is not a valid execution policy");

    return exec.copy_n(first, n, result);
  }

